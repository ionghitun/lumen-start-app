{
    "name": "ionghitun/lumen-start-app",
    "description": "Boilerplate for laravel lumen framework.",
    "keywords": [
        "ionghitun",
        "framework",
        "laravel",
        "lumen",
        "boilerplate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ion Ghitun",
            "email": "ionghitun@gmail.com"
        }
    ],
    "homepage": "https://github.com/ionghitun/lumen-start-app",
    "type": "project",
    "require": {
        "php": "^7.2",
        "illuminate/mail": "^6.8",
        "illuminate/redis": "^6.8",
        "intervention/image": "^2.5",
        "ionghitun/jwt-token": "^1.1",
        "ionghitun/laravel-lumen-mysql-encryption": "^1.1",
        "laravel/lumen-framework": "^6.2",
        "laravel/socialite": "^4.3",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
